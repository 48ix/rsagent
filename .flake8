[flake8]
max-line-length=88
count=True
show-source=False
statistics=True
exclude=.git, __pycache__
filename=*.py
per-file-ignores=rsagent/config.py:N805,E0213,R0903,E501,C0301
ignore=W503,C0330,R504,D202,PIE781,S404
select=B, BLK, C, D, E, F, I, II, N, P, PIE, S, R, W
disable-noqa=False
hang-closing=False
max-complexity=10
